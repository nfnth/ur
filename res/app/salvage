function updateInfo(i) { 
	
	if (domains[i].owner == "0x8a83fbbacb82030ea17179c0403b04e7bce7ba10") { M.toast({html: 'No owner found.'});
$("#o"+i).html("<img src='res/img/seal3.png' width='48' />");
 } else { 
